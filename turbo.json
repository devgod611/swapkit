{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "publish": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "test": {
      "inputs": [
        "__test__/**/*.ts",
        "__test__/**/*.tsx"
      ],
      "outputs": []
    },
    "test:coverage": {
      "inputs": [
        "__test__/**/*.ts",
        "__test__/**/*.tsx"
      ]
    }
  }
}
