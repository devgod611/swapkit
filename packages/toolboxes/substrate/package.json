    "@polkadot/util": "12.6.2"
  },
  "description": "SwapKit Lib substrate",
  "devDependencies": {
    "@internal/config": "workspace:*",
    "@swapkit/helpers": "workspace:*",
    "@swapkit/types": "workspace:*",
    "@vitest/coverage-istanbul": "1.1.0",
    "vite": "5.0.12",
    "vitest": "1.1.0"
  },
  "eslintConfig": {
    "extends": "../../../internal/eslint-config"
  },
  "exports": {
