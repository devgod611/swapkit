{
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/eslint-parser": "7.22.5",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.1",
    "@size-limit/file": "8.2.6",
    "@size-limit/time": "8.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "dotenv-cli": "7.2.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "size-limit": "8.2.6",
    "turbo": "1.10.6",
    "typescript": "5.1.6"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": true,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "pnpm": {
    "overrides": {
      "axios@^0.x.x": "^0.27.2",
      "json5@<=1.0.1": "^1.0.2",
      "semver@<=4.3.2": "^4.3.6"
    },
    "patchedDependencies": {
      "@ethersproject/abstract-signer@5.7.0": "patches/@ethersproject-abstract-signer-npm-5.7.0-f61d0a970e.patch",
      "cipher-base@1.0.4": "patches/cipher-base-npm-1.0.4-2e98b97140.patch",
      "sr25519@0.2.0": "patches/sr25519-npm-0.2.0-c9939c5c8a.patch",
      "typeforce@1.18.0": "patches/typeforce@1.18.0.patch",
      "asyncstorage-down@4.2.0": "patches/asyncstorage-down-npm-4.2.0-d323e014ba.patch",
      "@changesets/assemble-release-plan@5.2.3": "patches/@changesets__assemble-release-plan@5.2.3.patch",
      "@changesets/cli@2.26.1": "patches/@changesets__cli@2.26.1.patch"
    }
  },
  "scripts": {
    "bootstrap": "rm --rf node_modules; pnpm install; pnpm build",
    "build": "turbo run build --token=turbotoken-oss-europe1",
    "build:vite": "turbo run build-playground --token=turbotoken-oss-europe1 --concurrency=30",
    "preview:vite": "turbo run preview-playground --token=turbotoken-oss-europe1 --concurrency=30",
    "clean": "turbo run clean",
    "dev:android": "turbo run dev:android --concurrency=30 --filter=\"{./apps/expo-playground}...\"",
    "dev:expo": "turbo run dev --concurrency=30 --filter=\"{./apps/expo-playground}...\"",
    "dev:ios": "turbo run dev:ios --concurrency=30 --filter=\"{./apps/expo-playground}...\"",
    "dev:vite": "turbo run dev --concurrency=30 --filter=\"{./apps/vite-playground}...\"",
    "generate": "cd ./internal/generator; pnpm generate; cd ../..",
    "lint": "turbo run lint --token=turbotoken-oss-europe1",
    "postinstall": "rm -rf ./.git/hooks/pre-commit ./.git/hooks/prepare-commit-msg",
    "publish-packages": "turbo run build lint --token=turbotoken-oss-europe1 && pnpm version-bump && pnpm changeset publish",
    "test": "dotenv -- turbo run test --token=turbotoken-oss-europe1",
    "test:coverage": "dotenv -- turbo run test:coverage",
    "version-bump": "dotenv -- pnpm changeset version",
    "version-release": "pnpm changeset pre exit && pnpm version-bump"
  }
}
